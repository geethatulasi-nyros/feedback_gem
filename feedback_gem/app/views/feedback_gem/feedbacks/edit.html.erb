<div class="container new">
	<%= form_with(model: @feedback, local: true) do |f| %>
		<div>
			<p>
				<%=f.label :name %>
				<%=f.text_field :name %>
			</p>
			<p>
				<%=f.label :email %>
				<%=f.text_field :email %>
			</p>
			<p>
				<%=f.label :subject %>
				<%=f.text_field :subject %>
			</p>
			<p>
				<%=f.label :description %>
				<%=f.text_area :description %>
			</p>
			<%=link_to 'Back', feedbacks_path,:class=>'btn btn-secondary' %>
			<%=f.submit 'Update' ,:class=>'btn btn-warning'%>
		</div>
	<%end %>
</div>